    <script src="~/Areas/Admin/Scripts/shared/loading-screen.directive.js"></script>
    <script src="~/Areas/Admin/Scripts/review/group.filter.js"></script>
    <script src="~/Areas/Admin/Scripts/review/highlight.filter.js"></script>
    <script src="~/Areas/Admin/Scripts/review/start-from.filter.js"></script>
    <script src="~/Areas/Admin/Scripts/quizReview/question-status.filter.js"></script>

    <script src="~/Areas/Admin/Scripts/quizReview/quiz-review-data.service.js"></script>
    <script src="~/Areas/Admin/Scripts/review/review-data.service.js"></script>
    <script src="~/Areas/Admin/Scripts/student/student-data.service.js"></script>
    <script src="~/Areas/Admin/Scripts/quiz-details/quiz-details-data.service.js"></script>
    <script src="~/Areas/Admin/Scripts/quizzes/quizzes-data.service.js"></script>

    <script src="~/Areas/Admin/Scripts/quizReview/quiz-review.controller.js"></script>
    <script src="~/Areas/Admin/Scripts/review/review.controller.js"></script>
    <script src="~/Areas/Admin/Scripts/student/student.controller.js"></script>
    <script src="~/Areas/Admin/Scripts/quiz-details/quiz-details.controller.js"></script>
    <script src="~/Areas/Admin/Scripts/quizzes/quizzes.controller.js"></script>

    <link href="~/Areas/Admin/Styles/bootstrap-combined.css" rel="stylesheet" />
    <link href="~/Areas/Admin/Styles/equiz-temp.css" rel="stylesheet" />
