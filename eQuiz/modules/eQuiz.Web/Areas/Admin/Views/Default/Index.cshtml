@{
    Layout = "~/Views/Shared/Layout.cshtml";
}

@section HeadScripts
{
    <base href="/Admin/Default/" />
}

<div ng-app="equizModule">
        <div layout="row" style="padding:10px">
            <div>
                <a href="/Admin/Default/Index/">
                    <button style="text-decoration:solid; font-size:20px" ng-click="lc.GoToReview()">
                        Students
                    </button>
                </a>
                <a href="/Admin/Default/Index/Quizzes">
                    <button style="text-decoration:solid; font-size:20px">
                        Quizzes
                    </button>
                </a>
                        <label style="text-decoration:solid; font-size:20px; float:right">
                            Full Name
                        </label>
</div>

            <div data-loading class="cssload-container">
                <div class="cssload-speeding-wheel"></div>
            </div> 
            <div ng-view></div>
        </div>
</div>

@section FootScripts
